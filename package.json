{
  "name": "rstk-sfdx-package-utils",
  "description": "Utilities to better manage SalesforceDX Packages",
  "version": "0.0.2",
  "author": "John M. Daniel @ImJohnMDaniel",
  "bugs": "https://github.com/RootstockMFG/rstk-sfdx-package-utils/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/errors": "1",
    "cli-ux": "4.9.1",
    "inquirer": "6.2.0",
    "@salesforce/command": "0.1.6",
    "child-process-promise": "^2.2.1",
    "child_process": "^1.0.2",
    "tslib": "1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1",
    "@oclif/plugin-help": "1",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.1.0",
    "@types/chai": "4",
    "@types/jsforce": "1.8.13",
    "@types/mocha": "5",
    "@types/node": "10",
    "chai": "^4",
    "globby": "8",
    "mocha": "^5",
    "nyc": "^12",
    "sinon": "5",
    "ts-node": "6",
    "typescript": "2.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/RootstockMFG/rstk-sfdx-package-utils",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "rstk": {
        "description": "Commands to work with Rootstock ERP in SalesforceDX"
      },
      "rstk:package": {
        "description": "Commands focused on management of SalesforceDX package dependencies"
      },
      "rstk:package:dependencies": {
        "description": "Commands focused on management of SalesforceDX package dependencies"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "RootstockMFG/rstk-sfdx-package-utils",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
